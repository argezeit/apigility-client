{
    "name": "argezeit/apigility-client",
    "description": "Client for REST and RPC webservices",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "apigility",
        "rest",
        "zf2"
    ],
    "homepage": "https://github.com/argezeit/apigility-client",
    "support": {
        "email": "apigility-users@zend.com",
        "source": "https://github.com/argezeit/apigility-client",
        "issues": "https://github.com/argezeit/apigility-client/issues"
    },
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-servicemanager": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.2",
        "zendframework/zend-loader": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jolicht\\ApigilityClient\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JolichtTest\\ApigilityClient\\": "test/"
        }
    }
}
